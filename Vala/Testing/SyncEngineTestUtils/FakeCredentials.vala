
namespace Testing {

public class FakeCredentials : Occ.AbstractCredentials {

    Soup qnam;

    /***********************************************************
    ***********************************************************/
    public FakeCredentials (Soup qnam) {
        this.qnam = qnam;
    }

    /***********************************************************
    ***********************************************************/
    public override string authentication_type () {
        return "test";
    }

    /***********************************************************
    ***********************************************************/
    public override string user () {
        return "admin";
    }

    /***********************************************************
    ***********************************************************/
    public override string password () {
        return "password";
    }

    /***********************************************************
    ***********************************************************/
    public override Soup create_qnam () {
        return this.qnam;
    }

    /***********************************************************
    ***********************************************************/
    public override bool ready () {
        return true;
    }

    /***********************************************************
    ***********************************************************/
    public override void fetch_from_keychain () { }

    /***********************************************************
    ***********************************************************/
    public override void ask_from_user () { }

    /***********************************************************
    ***********************************************************/
    public override bool still_valid (Soup.Reply reply) {
        return true;
    }

    /***********************************************************
    ***********************************************************/
    public override void persist () { }

    /***********************************************************
    ***********************************************************/
    public override void invalidate_token () { }

    /***********************************************************
    ***********************************************************/
    public override void forget_sensitive_data () { }

}
}
