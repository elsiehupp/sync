        return true;
    }

    /***********************************************************
    ***********************************************************/
    public override void persist () { }

    /***********************************************************
    ***********************************************************/
    public override void invalidateToken () { }

    /***********************************************************
    ***********************************************************/
    public override void forgetSensitiveData () { }

}
}
