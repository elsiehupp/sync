    /***********************************************************
    ***********************************************************/
    public FakeCredentials (QNetworkAccessManager qnam) {
        this.qnam = qnam;
    }

    /***********************************************************
    ***********************************************************/
    public override string authentication_type () {
        return "test";
    }

    /***********************************************************
    ***********************************************************/
    public override string user () {
        return "admin";
    }

    /***********************************************************
    ***********************************************************/
    public override string password () {
        return "password";
    }

    /***********************************************************
    ***********************************************************/
